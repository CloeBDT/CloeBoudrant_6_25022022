{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "parser": "babel-eslint"
    },
    "rules": {
        "linebreak-style": 0,
        " global-require": 0,
        "eslint linebreak-style": [0, "error", "windows"]
    }
}
